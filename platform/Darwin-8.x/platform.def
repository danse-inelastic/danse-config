    PLATFORM_F77_FLAGS =
    PLATFORM_F77_DEFINES =
    PLATFORM_F77_INCLUDES = $(DIR_PLATFORM)/$(PLATFORM_ID)
    PLATFORM_LF77_FLAGS =
    PLATFORM_LF77_LIBPATH =

#
# OS-specific definitions
#
    EXT_SAR = dylib

    RANLIB = ranlib
    RANFLAGS =

    SYSLIB =

    TEX_DVIPDF = dvipdfm

