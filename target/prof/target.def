#
# Here are the build-specific flags for profiling
#
# C
#
    TARGET_CC_FLAGS += $(CC_PROF_FLAGS)
    TARGET_LCC_FLAGS += $(CC_PROF_FLAGS)
#
# C++
#
    TARGET_CXX_FLAGS += $(CXX_PROF_FLAGS)
    TARGET_LCXX_FLAGS += $(CXX_PROF_FLAGS)
#
# FORTRAN
#
    TARGET_F77_FLAGS += $(F77_PROF_FLAGS)
    TARGET_LF77_FLAGS += $(F77_PROF_FLAGS)
